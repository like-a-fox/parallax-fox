    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-spring": "^8.0.18",
    "styled-components": "^4.2.0",
    "tailwind.macro": "^0.5.10",
    "tailwindcss": "^1.1.2",
    "typeface-cantata-one": "^0.0.72",
    "typeface-open-sans": "^0.0.75"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "now": "^16.3.1",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "yarn build && yarn now --public",
    "develop": "gatsby develop",
