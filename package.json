  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/like-a-fox/parallax-fox"
  },
  "bugs": {
    "url": "https://github.com/like-a-fox/parallax-fox/issues"
