{
	"name": "parallax-fox",
	"description": "Parallax Like A Fox Portfolio",
	"version": "1.0.0",
	"author": "Derek Sligh<@like-a-fox>",
	"dependencies": {
		"babel-plugin-styled-components": "^1.10.0",
		"babel-plugin-tailwind-components": "^0.5.10",
		"dotenv": "^8.2.0",
		"firebase": "^7.2.2",
		"gatsby": "^2.17.4",
		"gatsby-image": "^2.2.29",
		"gatsby-plugin-google-analytics": "^2.1.23",
		"gatsby-plugin-manifest": "^2.2.20",
		"gatsby-plugin-offline": "^3.0.11",
		"gatsby-plugin-purgecss": "^4.0.1",
		"gatsby-plugin-react-helmet": "^3.1.10",
		"gatsby-plugin-sharp": "^2.2.28",
		"gatsby-plugin-smoothscroll": "^1.0.3",
		"gatsby-plugin-styled-components": "^3.0.7",
		"gatsby-plugin-tailwindcss": "^1.0.3",
		"gatsby-source-filesystem": "^2.1.29",
		"gatsby-transformer-sharp": "^2.2.20",
		"prop-types": "^15.7.2",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-helmet": "^5.2.1",
		"react-spring": "^8.0.18",
		"styled-components": "^4.4.0",
		"tailwindcss": "^1.1.3",
		"typeface-cantata-one": "^0.0.72",
		"typeface-open-sans": "^0.0.75"
	},
	"devDependencies": {
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.1.2",
		"now": "^16.5.2",
		"prettier": "^1.18.2"
	},
	"engines": {
		"node": ">=10"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"deploy": "gatsby clean && yarn build && firebase deploy",
		"now": "yarn deploy && now --public",
		"develop": "gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"serve": "gatsby serve",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true,
		"jsxSingleQuote": true,
		"endOfLine": "lf",
		"semi": true,
		"tabWidth": 1,
		"jsxBracketSameLine": true,
		"arrowParens": "always",
		"trailingComma": "es5"
	},
	"eslintConfig": {
		"globals": {
			"tw": true
		},
		"extends": [
			"react-app",
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"plugins": [
			"react-hooks"
		],
		"rules": {
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/like-a-fox/parallax-fox"
	},
	"bugs": {
		"url": "https://github.com/like-a-fox/parallax-fox/issues"
	}
}
