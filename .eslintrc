{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "settings": {
    "import/core-modules": [
      "react",
      "prop-types",
      "react-router-dom"
    ]
  },
  "globals": {
    "graphql": true
  },
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
